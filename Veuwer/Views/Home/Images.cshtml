@{
    ViewBag.Title = "Images";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="row toprow">
    <div class="col-md-12">
        <a href="/@(ViewBag.ImageUri).png">
            <img src="/@(ViewBag.ImageUri).png" class="userimage" />
